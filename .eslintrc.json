{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "google"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": ["error","single"],
        "linebreak-style": ["off", "windows"],
        "react/prop-types": "off",
        "require-jsdoc": "off",
        "no-invalid-this": "off",
        "no-unused-vars":"warn",
        "max-len": ["off", { "ignoreComments": true }],
        "new-cap": "off"
    }
}